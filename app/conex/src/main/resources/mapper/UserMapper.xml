<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.tttol.conex.mapper.UserMapper">
    <select id="findAll" resultType="io.github.tttol.conex.model.User">
        SELECT id, name
        FROM Users
        ORDER BY id
    </select>
</mapper>
